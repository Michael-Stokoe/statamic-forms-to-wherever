{
    "name": "stokoe/forms-to-wherever",
    "type": "statamic-addon",
    "description": "Base forms connector system for Statamic",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Stokoe",
            "email": "michael@stokoe.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Stokoe\\FormsToWherever\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stokoe\\FormsToWherever\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "statamic/cms": "^4.0 || ^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Forms To Wherever",
            "description": "Base forms connector system for Statamic"
        },
        "laravel": {
            "providers": [
                "Stokoe\\FormsToWherever\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
